{
  "name": "blinc",
  "version": "0.1.0",
  "description": "a nano web ramework",
  "main": "index.js",
  "files": [
    "/lib/*/blinc.js",
    "lib/*/blinc.min.js",
    "lib/*/tags/tags.js",
    "lib/*/tags/tags.min.js",
    "lib/*/tags/index.js",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "blinc-min": "terser build/blinc.js -o build/blinc.min.js -c -m",
    "tags-min": "terser build/tags/tags.js -o build/tags/tags.min.js -c -m",
    "blinc-compile": "babel src/index.js  --out-file build/blinc.js",
    "tags-compile": "babel src/tags/index.js  --out-file build/tags/tags.js",
    "build": "npm run blinc-compile && npm run tags-compile && npm run blinc-min && npm run tags-min",
    "start": "npx babel src/index.js --watch --out-file lib/blinc.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "terser": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamlesh-nb/blinc.git"
  },
  "bugs": {
    "url": "https://github.com/kamlesh-nb/blinc.git/issues"
  },
  "keywords": [
    "blinc",
    "function ui",
    "framework"
  ],
  "author": "kamlesh bambarde",
  "license": "MIT",
  "homepage": "https://github.com/kamlesh-nb",
  "dependencies": {
    "firebase": "^7.14.6"
  }
}
